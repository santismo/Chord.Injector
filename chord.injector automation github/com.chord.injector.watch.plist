<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>com.chord.injector.watch</string>
    <key>ProgramArguments</key>
    <array>
      <string>/usr/bin/env</string>
      <string>node</string>
      <string>/Users/YOUR_USERNAME/PATH_TO/chord.injector automation/process-midi.js</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <true/>
    <key>StandardOutPath</key>
    <string>/Users/YOUR_USERNAME/PATH_TO/chord.injector automation/watch.log</string>
    <key>StandardErrorPath</key>
    <string>/Users/YOUR_USERNAME/PATH_TO/chord.injector automation/watch.log</string>
  </dict>
</plist>
